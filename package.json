{
  "name": "cms-boilerplate",
  "version": "0.1.5",
  "description": "CMS boilerplate for sites/blogs/landings",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:codecov": "codecov -f ./coverage/coverage-final.json",
    "lint": "eslint --ext .ts,.tsx src/** stories/**",
    "lint:fix": "eslint --ext .ts,.tsx --fix src/** stories/**",
    "lint:report-html": "eslint --ext .ts,.tsx -f html -o ./reports/eslint.html src/** stories/**",
    "lint:report-junit": "eslint --ext .ts,.tsx -f junit -o ./reports/eslint.xml src/** stories/**",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AntonLukichev/cms.git"
  },
  "keywords": [],
  "author": {
    "name": "Anton Lukichev",
    "email": "anton@lukichev.pro"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AntonLukichev/cms/issues"
  },
  "homepage": "https://github.com/AntonLukichev/cms#readme",
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "clsx": "^1.1.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.4.1",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-loader": "^8.1.0",
    "codecov": "^3.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^25.5.4",
    "jest-junit": "^10.0.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2"
  }
}
